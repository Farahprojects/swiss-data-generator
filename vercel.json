{
  "rewrites": [
    
    {
      "source": "/report/:path*",
      "has": [
        { "type": "host", "value": "www.theraiastro.com" }
      ],
      "destination": "/report/:path*"
    },

    
    {
      "source": "/:path*",
      "has": [
        {
          
          "type": "host",
          "value": "(?<slug>[^.]+)\\.theraiastro\\.com"
        }
      ],
      
      "destination": "/site?slug=:slug&path=:path*"
    }
  ]
}
