{
  "rewrites": [
    {
      "source": "/",                 
      "has": [
        { "type": "host", "value": "www.theraiastro.com" }
      ],
      "destination": "/report"       
    },
    {
      "source": "/:path*",          
      "has": [
        { "type": "host", "value": "www.theraiastro.com" }
      ],
      "destination": "/report/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}
}
