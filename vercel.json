
{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    },
    {
      "source": "/bimi-logo.svg",
      "headers": [
        { "key": "Content-Type", "value": "image/svg+xml" }
      ]
    }
  ]
}
